<%- include('./layout/header') %>
    <%- include('./components/navbar') %>
        <main>
            <div class="container-fluid">
                <div class="row">
                    <%- include('./components/aside') %>
                        <div class="col-md-10 bg-light py-3">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="bg-white rounded shadow mb-3">
                                        <div class="uk-h4 my-0 p-3 border-bottom border-muted point-control" uk-toggle="target: #checkbox-default">
                                            Checkbox Default
                                            <span class="fa fa-angle-down float-right"></span>
                                        </div>
                                        <div id="checkbox-default" class="d-block p-3">
                                            <div class="form-check">
                                                <input type="checkbox" class="form-check-input" id="exampleCheck1">
                                                <label class="form-check-label" for="exampleCheck1">Bootstrap Default Checkbox</label>
                                            </div>
                                            <div class="form-group">
                                                <label>
                                                    <input class="uk-checkbox" type="checkbox" checked> UIKit default Checkbox
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="bg-white rounded shadow mb-3">
                                        <div class="uk-h4 my-0 p-3 border-bottom border-muted point-control" uk-toggle="target: #radio-custom">
                                            Select Default
                                            <span class="fa fa-angle-down float-right"></span>
                                        </div>
                                        <div id="radio-custom" class="d-block p-3">
                                            <div class="form-check">
                                                <input type="radio" class="form-check-input" id="exampleradio">
                                                <label class="form-check-label" name="exampleradio" for="exampleradio">Bootstrap Default Checkbox</label>
                                            </div>
                                            <div class="form-group">
                                                <label>
                                                    <input id="exampleradio2" name="exampleradio" class="uk-radio" type="radio" checked> UIKit default Checkbox
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                </div>
            </div>
        </main>

        <%- include('./layout/footer') %>